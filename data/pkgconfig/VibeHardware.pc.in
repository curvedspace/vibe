prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
bindir=${prefix}/bin
libdir=${prefix}/lib
includedir=${prefix}/include
qtminver=@QT_MIN_VERSION@

Name: VibeHardware
Description: Hardware abstraction library for Vision applications
Version: @VIBE_VERSION_STR@
Libs: -L${libdir} -lVibeHardware
Requires: QtCore >= @QT_MIN_VERSION@, QtDBus >= @QT_MIN_VERSION@, QtXml >= @QT_MIN_VERSION@, QtGui >= @QT_MIN_VERSION@
Cflags: -I${includedir}
