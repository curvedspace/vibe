prefix=@CMAKE_PREFIX@
exec_prefix=${prefix}
bindir=@QUBE_SYSTEM_BINARY_DIR@
etcdir=@QUBE_SETTINGS_DIR@
libdir=@QUBE_SYSTEM_LIBRARY_DIR@
includedir=@QUBE_DEVELOPMENT_INCLUDE_DIR@
