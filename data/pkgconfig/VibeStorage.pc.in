prefix=@CMAKE_INSTALL_SYSTEMDIR@
exec_prefix=${prefix}
bindir=@CMAKE_INSTALL_BINDIR@
libdir=@CMAKE_INSTALL_LIBDIR@
includedir=@CMAKE_INSTALL_INCLUDEDIR@
qtminver=@QT_MIN_VERSION@

Name: VibeStorage
Description: Storage utility library for Vision applications
Version: @VIBE_VERSION_STRINGING@
Libs: -L${libdir} -lVibeStorage
Requires: QtXml >= @QT_MIN_VERSION@, QtGui >= @QT_MIN_VERSION@, VibeCore = @VIBE_VERSION_STRINGING@, VibeHardware = @VIBE_VERSION_STRINGING@
Cflags: -I${includedir} -I${includedir}/VibeStorage
