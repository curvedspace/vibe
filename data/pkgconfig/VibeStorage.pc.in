prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
bindir=${prefix}/bin
libdir=${prefix}/lib
includedir=${prefix}/include
qtminver=@QT_MIN_VERSION@

Name: VibeGui
Description: VibeGui Library
Version: @VIBE_VERSION_STR@
Libs: -L${libdir} -lVibeStorage
Requires: QtGui >= @QT_MIN_VERSION@, QtXml >= @QT_MIN_VERSION@, VibeCore = @VIBE_VERSION_STR@, VibeHardware = @VIBE_VERSION_STR@
Cflags: -I${includedir}
